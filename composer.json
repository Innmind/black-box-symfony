{
    "name": "innmind/black-box-symfony",
    "type": "library",
    "description": "Extension to test Symfony apps via BlackBox",
    "keywords": ["testing", "symfony"],
    "homepage": "http://github.com/innmind/black-box-symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/innmind/black-box-symfony/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/black-box": "~5.3|^6.0.2",
        "innmind/json": "~1.4",
        "symfony/framework-bundle": "~6.0|~7.0",
        "symfony/browser-kit": "~6.0|~7.0",
        "symfony/http-foundation": "~6.0|~7.0",
        "symfony/http-kernel": "~6.0|~7.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\BlackBox\\Symfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\BlackBox\\Symfony\\": "tests/",
            "App\\": "fixtures/"
        }
    },
    "require-dev": {
        "innmind/static-analysis": "^1.2.1",
        "innmind/coding-standard": "~2.0"
    },
    "provide": {
        "innmind/black-box-extension": "5.0"
    }
}
